#!/usr/bin/env python

NODE_NAME = 'move_control'

def setup_pubs_subs():
    for addr in mac_addrs:
        pub = rospy.Publisher(addr + '/cmd', String, queue_size=10)
        sub = rospy.Subscriber(addr + '/mag', Int32, queue_size=10)

def sub_sensors():
    for addr in mac_addrs:
        sub = rospy.Subscriber()
    pass

def pub_cmds():

def main():
    sub_sensors()
    pub_cmds()
    
    rospy.init_node(NODE_NAME, anonymous=FALSE)
    rate = rospy.Rate(10)
    while not rospy.is_shutdown():
        
        rate.sleep()
    
